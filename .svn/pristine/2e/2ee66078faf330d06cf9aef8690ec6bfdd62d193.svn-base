public enum TipusPagament {

    EFECTIU("E"),
    TARGETA("T"),
    TRANSFERENCIA("TF"),
    FINANSAMENT("FF");

    private String codi;

    TipusPagament(String codi) {
        this.codi = codi;
    }

    public String getCodi() {
        return codi;
    }

    public void setCodi(String codi) {
        this.codi = codi;
    }
}
